$panel = $('#section-layout-panel');
$panel.destroySelectized();
$panel.replaceWith(
  '<%= j render partial: 'admin/sections/panels/layout', object: @section, as: :section %>'
);
$('#<%= dom_id(@section) %> > .section').replaceWith(
  '<%= j render @section %>'
);
$('#<%= dom_id(@section) %> > .section').addClass('active');
$('#<%= dom_id(@section) %>').initialize();
