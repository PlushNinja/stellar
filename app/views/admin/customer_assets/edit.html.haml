.page-header
  = link_to admin_customer_assets_path, class: 'btn btn-default' do
    = icon('chevron-circle-left', t('actions.back'))
  = link_to admin_customer_asset_path(@customer_asset), class: 'btn btn-primary' do
    = icon('info-circle', t('actions.show'))
  %h1
    = icon('archive', @customer_asset.product.title)
    %span.small= @customer_asset.user

= render partial: 'admin/asset_entries/edit_table', object: @customer_asset, as: :customer_asset
