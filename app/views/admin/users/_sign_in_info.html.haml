= bootstrap_form_for [:admin, @user] do |f|
  .panel.panel-default
    %fieldset.panel-body{disabled: true}
      = f.text_field :last_sign_in_at, value: @user.last_sign_in_at.present? ? l(@user.last_sign_in_at, format: :iso) : nil
      = f.text_field :last_sign_in_ip
      = f.text_field :sign_in_count
