$("#{j render partial: 'admin/images/edit', object: @image, as: :image}").appendTo('##{dom_id(@imageable, :images)}');
$("##{dom_id(@image)}").initialize();
$('##{dom_id(@imageable, :images)}').sortable('refresh');
$('##{dom_id(@imageable, :images)}').imagesLoaded(function () { $('##{dom_id(@imageable)}').trigger('imageable:added'); });
