%li.dropdown.mega-dropdown
  %a.dropdown-toggle{href: '#', data: {toggle: 'dropdown'}}
    = root
    %span.caret
  %ul.dropdown-menu.mega-dropdown-menu{role: 'menu'}
    .container
      .row
        = render partial: "#{partial_path}/megamenu_item", collection: items, as: :item, locals: {root: root}
